{
  "name": "qiankun-quick-start",
  "version": "1.0.0",
  "description": "qiankun 应用",
  "main": "index.js",
  "scripts": {
    "--------用途": "下载所有的依赖",
    "install": "npm-run-all --serial install:*",
    "install:main": "cd main && yarn",
    "install:vue-app": "cd vue-app && yarn",
    "--------项目启动": "项目启动~",
    "dev": "npm-run-all --parallel dev:*",
    "dev:vue-app": "cd vue-app && yarn serve",
    "dev:main": "cd main && yarn serve",
    "--------打包": "项目打包~",
    "build": "npm-run-all build:*",
    "build:vue-app": "cd vue-app && npm run build",
    "build:main": "cd main && npm run build"
  },
  "keywords": [
    "qiankun",
    "qiankun-quick-start"
  ],
  "author": "云层上的光|初心",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
